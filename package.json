{
  "name": "onboarder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:client": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "migrate:dev": "prisma migrate dev",
    "migrate:resolve": "prisma migrate resolve",
    "migrate:status": "prisma migrate status",
    "migrate:diff": "prisma migrate diff",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "lint": "next lint",
    "pretest": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test": "dotenv -e .env.test -- jest -i",
    "email:preview": "email dev -p 3001"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^5.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "18.2.7",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "postmark": "^3.0.19",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.15",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "eslint-config-next": "^13.4.19",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "prisma": "^5.2.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
